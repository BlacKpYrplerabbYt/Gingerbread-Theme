<?xml version="1.0" encoding="utf-8"?>
<resources>
<!-- Theme properties -->
<string name="theme_title">Gingerbread Theme</string>
<!-- To devs, read here on howto format/style your descriptions -->
<!-- http://developer.android.com/intl/de/guide/topics/resources/string-resource.html#FormattingAndStyling -->
<string name="theme_description">This is a beautiful theme for ADW Launcher.&lt;br/>
	Keep up to date with the latest info through &lt;a href=http://www.twitter.com/yourname>Twitter&lt;/a></string>
<!-- Theme specific appearance settings -->
<color name="bubble_color">#b2191919</color>
<color name="bubble_text_color">#FFFFFFFF</color>
<color name="drawer_text_color">#FFFFFFFF</color>
<integer name="bubble_radius">8</integer>
<color name="folder_title_color">#FFCCCCCC</color>
<bool name="use_drawer_icons_bg">false</bool>
<color name="desktop_indicator_color">#46586991</color>
<!-- ADWSettings theme config  -->
<bool name="config_uiABBg">false</bool>
<integer name="config_highlights_color">0xffffb600</integer>
<integer name="config_highlights_color_focus">0xffff8a12</integer>
<bool name="config_new_selectors">false</bool>
<integer name="config_drawer_color">0xff000000</integer>
<bool name="config_drawerLabels">true</bool>
<bool name="config_fadeDrawerLabels">false</bool>
<bool name="config_desktop_indicator">false</bool>
<!-- 1=top dots, 2=top slider, 3=bottom slider -->
<string name="config_desktop_indicator_type">1</string>
<!-- config_ab_scalefactor is a float value between 0.1 and 0.8. Set here the default .X (-1, 6 will become 7) -->
<integer name="config_ab_scale_factor">6</integer>
<!-- Force the theme to use an specific dock style -->
<!--  0=None, 1=3buttons, 2=5buttons, 3=1button -->
<string name="main_dock_style">2</string>
<!-- remove this line if you don't want it to show -->
<string name="authorName">Haxzamatic</string>
<!-- remove this line if you don't want the market link -->
<string name="developerName">GFX Android</string>
<!-- remove this line if you don't have one -->
<string name="authorLink">http://haxzamatic.com</string>
<bool name="widget_calendar_calendarview_day_drawborders">false</bool>
</resources>
